MICRO
m00 00000 000 000 111 000 011 0000 0001 00 0111010111 c//Bootstrap
m01 00000 000 000 000 000 001 0000 0000 00 0010000000 c//Bootstrap
m02 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDA #K PC+1->RC,MAR
m03 00000 000 000 111 000 011 0000 0000 00 1110011101 c//MDR+0->ACC
m04 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m05 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m06 00000 000 000 101 000 011 0001 0001 01 1111011110 c//STA $K PC+1->PC,MAR
m07 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+0->NOP,MAR
m08 00000 000 000 100 000 001 0000 0000 00 1000011111 c//ACC+0->NOP,MWE~
m09 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m0a 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m0b 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDX #K PC+1->RC,MAR
m0c 00000 000 000 111 000 011 0000 0010 00 1110011101 c//MDR+0->X
m0d 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->RC,MAR
m0e 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m0f 00000 000 000 101 000 011 0001 0001 01 1111011110 c//STX $K PC+1->PC,MAR
m10 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+0->NOP,MAR
m11 00000 000 000 100 000 001 0010 0000 00 1000011111 c//X+0->NOP,MWE~
m12 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m13 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m14 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDA ($K) PC+1->PC,MAR
m15 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+0->NOP,MAR
m16 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+0->NOP,MAR
m17 00000 000 000 111 000 011 0000 0000 00 1110011101 c//MDR+0->ACC
m18 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m19 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m1a 00000 000 000 100 000 001 0010 0000 00 1011011111 c//LDAX X+0->NOP,MAR
m1b 00000 000 000 111 000 011 0000 0000 00 1110011101 c//MDR+0->ACC
m1c 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m1d 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m1e 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDA $K,X PC+1->PC,MAR
m1f 00000 000 000 101 000 001 0010 0000 00 1011011101 c//MDR+X->NOP,MAR
m20 00000 000 000 111 000 011 0000 0000 00 1110011101 c//MDR+0->ACC
m21 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m22 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m23 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDA ($K,X) PC+1->PC,MAR
m24 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+0->NOP,MAR
m25 00000 000 000 101 000 001 0010 0000 00 1011011101 c//MDR+X->NOP,MAR
m26 00000 000 000 111 000 011 0000 0000 00 1110011101 c//MDR+0->ACC
m27 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m28 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)

MAPPER
m00 02 c//LDA #K
m01 06 c//STA $K
m02 0b c//LDX #K
m03 0f c//STX $K
m04 14 c//LDA ($K)
m05 1a c//LDAX
m06 1e c//LDA $K,X
m07 23 c//LDA ($K,X)

MAIN
