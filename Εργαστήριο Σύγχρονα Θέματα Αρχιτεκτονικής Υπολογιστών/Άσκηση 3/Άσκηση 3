MICRO
m00 00000 000 000 111 000 011 0000 0001 00 0111010111 c//Bootstrap
m01 00000 000 000 000 000 001 0000 0000 00 0010000000 c//Bootstrap									
m02 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDA $K,X, PC+1->PC,MAR												
m03 00000 000 000 101 000 001 0010 0000 00 1011011101 c//MDR+X->NOP,MAR																		
m04 00000 000 000 111 000 011 0000 0000 00 1110011101 c//MDR+0->ACC																		
m05 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR											
m06 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)									
m07 00000 000 000 101 000 011 0001 0001 01 1111011110 c//LDX #K, PC+1->PC,MAR
m08 00000 000 000 111 000 011 0000 0010 00 1110011101 c//MDR+0->X																	
m09 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m0a 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m0b 00000 000 000 101 000 011 0010 0010 01 1110011110 c//ÉÍ×, X+1->X																		
m0c 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m0d 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m0e 00000 000 000 101 000 011 0001 0001 01 1111011110 c//CMPX #Y, PC+1->PC,MAR																		
m0f 00000 000 000 101 001 001 0010 0000 00 1010111101 c//X-MDR->NOP,FLAGS																	
m10 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m11 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m12 00000 000 000 101 000 011 0001 0001 01 1111011110 c//STA $K,X, PC+1->PC,MAR
m13 00000 000 000 101 000 001 0010 0000 00 1011011101 c//MDR+X->NOP,MAR
m14 00000 000 000 100 000 001 0000 0000 00 1000011111 c//ACC+0->NOP,MWE~
m15 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m16 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m17 00000 000 000 101 000 011 0001 0001 01 1111011110 c//ADC $K,X, PC+1->PC,MAR
m18 00000 000 000 101 000 001 0010 0000 00 1011011101 c//MDR+X->NOP,MAR
m19 00000 000 000 101 000 011 0000 0000 00 1110111001 c//MDR+ACC+Cin->ACC,FLAGS
m1a 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m1b 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m1c 00000 000 000 111 000 001 0000 0000 01 1010111110 c//CRC, 1+0->NOP,FLAGS
m1d 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m1e 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m1f 00100 011 011 000 000 001 0000 0000 00 1010011111 c//BRNZ $K, BRZ offset
m20 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m21 00000 000 000 111 000 011 0000 0001 00 1111011101 c//MDR+0->PC,MAR
m22 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m23 00000 000 000 101 000 011 0001 0001 10 1111011110 c//PC+2->PC,MAR
m24 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m25 00000 000 000 100 000 111 0000 0000 00 0110011111 c//SHLA, ACC:SL->ACC
m26 00000 000 000 101 000 011 0001 0001 01 1111011110 c//PC+1->PC,MAR
m27 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m28 00000 000 000 101 000 011 0001 0001 00 1111011110 c//HALT, PC+0->PC,MAR
m29 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)

MAPPER 
m00 02 c//LDA $K,X 
m01 07 c//LDX #K 
m02 0b c//INX
m03 0e c//CMPX #Y
m04 12 c//STA $K,X
m05 17 c//ADC $K,X
m06 1c c//CRC
m07 1f c//BRNZ $K
m08 25 c//SHLA 
m09 28 c//HALT

