MICRO


m00 00000 000 000 111 000 011 0000 0001 00 0111010111 c//Bootstrap

m01 00000 000 000 000 000 001 0000 0000 00 0010000000 c//Bootstrap

m02 00000 000 000 101 000 011 0001 0001 01 0111011110 c//LDA $K PC+1->PC,MAR
m03 00000 000 000 111 000 001 0000 0000 00 1011011101
 c//MDR+0->NOP,MAR
m04 00000 000 000 111 000 011 0000 0000 00 0110011101 c//MDR+0->ACC
m05 00000 000 000 101 000 011 0001 0001 01 0111011110 c//PC+1->PC,MAR
m06 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC)
m07 00000 000 000 101 000 011 0001 0001 01 0111011110 c//ADD $K PC+1->PC,MAR

m08 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+ACC->ACC
m09 00000 000 000 101 000 011 0000 0000 00 0110011101 c//ACC+0->NOP,MWE~
m0a 00000 000 000 101 000 011 0001 0001 01 0111011110
 c//PC+1->PC,MAR
m0b 00000 000 000 000 000 001 0000 0000 00 0010000000
 c//NEXT(PC)
m0c 00000 000 000 101 000 011 0001 0001 01 0111011110 c//STA $K PC+1->PC,MAR

m0d 00000 000 000 111 000 001 0000 0000 00 1011011101 c//MDR+0->NOP,MAR
m0e 00000 000 000 100 000 001 0000 0000 00 0100011111 c//ACC+0->NOP,MWE~
m0f 00000 000 000 101 000 011 0001 0001 01 0111011110
 c//PC+1->PC,MAR
m10 00000 000 000 000 000 001 0000 0000 00 0010000000 c//NEXT(PC) 




MAPPER 		

m00 02	c//LDA $K
m01 07	c//ADD $K
m02 0c	c//STA
 $K


MAIN

m00 00	c//opcode LDA $K
m01 08	c//entelo LDA $K
m02 01	c//opcode ADD $K
m03 09	c//entelo ADD $K
m04 02	c//opcode STA
 $K
m05 0a	c//entelo STA
 $K

